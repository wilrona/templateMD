<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
    <meta charset="{{site.charset}}" />
        <title>
            {% if function('is_category') %}
                {{  function('single_cat_title') }}
            {% elseif function('is_search') %}
                Recherche pour &quot; {{ function('wp_specialchars', s) }} &quot; | {{ site.name }}
            {% elseif function('is_home') or function('is_front_page') %}
                {{ site.name }} | {{ site.description }}
            {% elseif function('is_404') %}
                Error 404 Not Found | {{ site.name }}
            {% elseif function('is_single') %}
                {{ wp_title }} | {{ site.name }}
            {% else %}
                {{ wp_title }} | {{ site.name }}
            {% endif %}
        </title>
{#    <meta name="description" content="{{site.description}}">#}
    <link rel="stylesheet" href="{{site.theme.link}}/style.css" type="text/css" media="screen" />
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="pingback" href="{{site.pingback_url}}" />

    <meta name="google-site-verification" content="XNeQyb43rhmWySvwFs1tbKYp82-2HCmYQAOBphV4FHc" />

    <link rel="icon" type="image/png" href="{{ Image(options.icon) }}" />

    <link href='https://fonts.googleapis.com/css?family=Montserrat:400,600,700%7CSource+Sans+Pro:400,600,700' rel='stylesheet'>

    {{function('wp_head')}}

